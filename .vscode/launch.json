  "version": "0.2.0",
  "configurations": [
    {
      "name": "04-downloadPlaceId",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--require=dotenv/config",
        "--nolazy",
        "-r",
        "ts-node/register/transpile-only"
      ],
      "args": ["04-downloadPlaceId/index.ts"],
      "cwd": "${workspaceRoot}/data",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    },
    {
      "name": "05-downloadDetails",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--require=dotenv/config",
        "--nolazy",
        "-r",
        "ts-node/register/transpile-only"
      ],
      "args": ["data/05-downloadDetails/index.ts"],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
